<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
  <persistence-unit name="IED_Model">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>jdbc/orcl_testDS</jta-data-source>
    <class>com.minhdt.model.entities.SysParam</class>
    <class>com.minhdt.model.entities.DicItemDefine</class>
    <class>com.minhdt.model.entities.SysFunction</class>
    <class>com.minhdt.model.entities.SysTask</class>
    <class>com.minhdt.model.entities.SysFunctionTask</class>
    <class>com.minhdt.model.entities.SysGroup</class>
    <class>com.minhdt.model.entities.SysGroupFunction</class>
    <class>com.minhdt.model.entities.SysGroupFunctionTask</class>
    <class>com.minhdt.model.entities.SysUser</class>
    <class>com.minhdt.model.entities.SysGroupUser</class>
    <properties>
      <property name="eclipselink.target-server" value="WebLogic_10"/>
      <property name="javax.persistence.jtaDataSource" value="jdbc/orcl_testDS"/>
      <property name="eclipselink.cache.shared.default" value="false"/>
    </properties>
  </persistence-unit>
</persistence>
